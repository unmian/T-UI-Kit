{
  "name": "t-ui-kit",
  "version": "0.1.0",
  "author": {
    "name": "Quarter",
    "email": "master@shiruihua.com.cn"
  },
  "description": "UI Kit for TDesign Community",
  "keywords": [
    "vue3",
    "TDesign Community",
    "UI Kit"
  ],
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/unmian/T-UI-Kit"
  },
  "files": [
    "lib",
    "types"
  ],
  "main": "lib/index.umd.js",
  "module": "lib/index.es.js",
  "exports": {
    ".": {
      "import": "./lib/index.es.js",
      "require": "./lib/index.umd.js"
    }
  },
  "typings": "types/index.d.ts",
  "homepage": "https://github.com/unmian/T-UI-Kit",
  "bugs": "https://github.com/unmian/T-UI-Kit/issues",
  "scripts": {
    "dev": "vite --config ./build/base.config.ts",
    "test": "vue-tsc --declaration --emitDeclarationOnly",
    "build:lib": "vue-tsc --noEmit && vite build --config ./build/lib.config.ts",
    "build:doc": "vue-tsc --noEmit && vite build --config ./build/doc.config.ts",
    "preview": "vite preview",
    "publish:private": "npm publish --tag next --registry https://npm.lescity.com.cn/"
  },
  "dependencies": {
    "vue": "^3.2.25",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@types/node": "^17.0.5",
    "@vitejs/plugin-legacy": "^1.6.4",
    "@vitejs/plugin-vue": "^2.0.0",
    "autoprefixer": "^10.4.1",
    "postcss": "^8.4.5",
    "sass": "^1.45.1",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vite-plugin-dts": "^0.9.6",
    "vite-plugin-md": "^0.11.7",
    "vue-tsc": "^0.29.8"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "ie >= 10",
    "iOS >= 8",
    "Android >= 4"
  ]
}